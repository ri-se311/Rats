                  <Font name="Britannic Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="mainMenuHighScoreButton" layoutX="10.0" layoutY="114.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="HighScore">
