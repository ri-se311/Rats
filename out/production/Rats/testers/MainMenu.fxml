            <Button fx:id="mainMenuPlayButton" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="Play">
               <font>
                  <Font name="Britannic Bold" size="12.0" />
               </font>
               <effect>
                  <Glow />
               </effect>
            </Button>
            <Button fx:id="mainMenuNewProfileButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="New Profile">
