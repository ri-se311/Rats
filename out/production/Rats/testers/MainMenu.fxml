                  <Glow />
               </effect>
               <font>
                  <Font name="Britannic Bold" size="12.0" />
               </font>
            </Button>
         </children>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </VBox>
      <Button fx:id="mainMenuQuitButton" layoutX="636.0" layoutY="320.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="100.0" text="Quit">
