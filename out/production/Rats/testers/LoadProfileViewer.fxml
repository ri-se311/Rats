      <Label text="Profiles" BorderPane.alignment="CENTER">
         <font>
            <Font size="48.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
   </top>
