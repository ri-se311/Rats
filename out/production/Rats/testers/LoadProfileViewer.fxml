      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="loadProfileBackButton" mnemonicParsing="false" prefHeight="21.0" prefWidth="60.0" text="Back">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
